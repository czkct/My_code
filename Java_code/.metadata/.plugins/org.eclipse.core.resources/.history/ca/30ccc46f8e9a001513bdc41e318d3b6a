import java.awt.*;
public class Blood {
	int x, y, w, h;
	TankClient tc;
	
	private int[][] = {
			{350,300},{370,300},{375,275},{400,200},{360,270},{365,290},{350,280}
	};
	
	public void draw(Graphics g){
		Color c = g.getColor();
		g.setColor(Color.magenta);
		g.fillRect(x, y, w, h);
		g.setColor(c);
	}
}
